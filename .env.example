# ============================================================================
# AGENTGUARD - ENVIRONMENT VARIABLES
# ============================================================================
#
# Copy this file to .env and set your values
#
# ============================================================================

# Admin Key (REQUIRED for production)
# This key is used to create customers and manage the platform
# Generate with: openssl rand -hex 32
AGENTGUARD_ADMIN_KEY=your-secret-admin-key-here

# Environment
AGENTGUARD_ENV=production

# Port
AGENTGUARD_PORT=5000

# Log level
AGENTGUARD_LOG_LEVEL=INFO

# Database path (inside container)
AGENTGUARD_DB_PATH=/app/data/agentguard.db
